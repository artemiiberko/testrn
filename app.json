{
  "expo": {
    "name": "testrn",
    "slug": "testrn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/saluderia-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/saluderia-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.saluderia",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/saluderia-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
